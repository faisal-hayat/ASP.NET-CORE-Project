<!-- Need to tell what kind of data is coming here explicitly -->
@model IEnumerable<Category> 

@{
    ViewData["Title"] = "Index";
}
<!-- This is Category Home View -->
<h1>
    Category
</h1>
<table class="table table-bordered table-striped" style="width: 100%">
    <thead>

        <tr>

            <th>
                Category Name
            </th>

            <th>
                Display Order
            </th>

        </tr>

    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>

                <td width="30%">
                    @obj.DisplayOrder
                </td>

            </tr>

        }
    </tbody>
</table>